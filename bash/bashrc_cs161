source $HOME/dotfiles/bash/bashrc_global

source $HOME/dotfiles/bash/aliases_cs161

# make less more friendly for non-text input files, see lesspipe(1)
[ -x /usr/bin/lesspipe ] && eval "$(SHELL=/bin/sh lesspipe)"

export PATH=$HOME/cs161/sys161/bin:$HOME/cs161/tools/bin:$HOME/cs161/root/hostbin:$PATH

root=$HOME/cs161/root

cd $HOME/cs161/os161
